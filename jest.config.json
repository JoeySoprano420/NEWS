{
  "testEnvironment": "node",
  "transform": {},
  "testMatch": [
    "**/*.test.js",
    "**/*.spec.js"
  ],
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 70,
      "lines": 70,
      "statements": 70
    }
  }
}